# API ключ для AITunnel (для доступа к DeepSeek)
# Получите ключ на https://aitunnel.ru
OPENAI_API_KEY=your_aitunnel_api_key_here

# Модель DeepSeek для использования
# По умолчанию: deepseek-chat (DeepSeek v3.2)
DEEPSEEK_MODEL=deepseek-chat

# Neo4j Database Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# Год данных по умолчанию
DEFAULT_YEAR=2024

# Redis Configuration (для Bull Queue)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Server Configuration
PORT=3000
NODE_ENV=development

# API Key для защиты эндпоинтов
API_KEY=your_secure_api_key_here